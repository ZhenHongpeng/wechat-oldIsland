/* components/like/index.wxss */
.container {
    /*保持宽度自适应使用inline-flex*/
    display: inline-flex;
    flex-direction: row;
    padding: 10rpx;
    width: 90rpx;
}
.container image{
    width: 32rpx;
    height: 28rpx;
}
.container text {
    font-size: 24rpx;
    color: #bbbbbb;
    position: relative;
    bottom: 10rpx;
    left: 6rpx;
    line-height: 24rpx;
}